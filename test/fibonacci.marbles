
fn fibonacciA num
	n num
	? (= n 0) 0 (? (= n 1) 1 (fibonacci (- n 1) (- n 2) ) )


fn fibonacciB num
	n num
	?
		= n 0
		0
		?
			= n 1
			1
			fibonacci
				- n 1
				- n 2


@enter

state fibCounterAout num
	fibonacciA currentFrame

state fibCounterBout num
	fibonacciB currentFrame

var exitAt num
	12

nextFrameform
	? (> curFrame exitAt) exit enter

