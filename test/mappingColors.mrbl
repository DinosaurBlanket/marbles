
frameRate
	10
videoWidth
	512
videoHeight
	512


fn redFiller B4
	xPos N1
	yPos N1
	sourceWidth  N1
	sourceHeight N1
	B4from01
		/
			+
				/ xPos sourceWidth
				/ yPos sourceHeight
			3
		0
		0
		1

fn cyanFiller B4
	xPos N1
	yPos N1
	sourceWidth  N1
	sourceHeight N1
	B4from01
		0
		/ xPos sourceWidth
		/ yPos sourceHeight
		1

var image B4D2
	mapInB4D2
		redFiller
		N4 -40 -40 80 80
		mapB4D2
			cyanFiller
			buildB4D2
				videoWidth
				videoHeight


@enter

videoOut
	image

